#include<stdio.h>
void main()
{
    int a,b,result;
    float div;
    printf("Enter two values:");
    scanf("%d",&a);
    scanf("%d",&b);
    result=a+b;
    printf("sum=%d&%d\n",a,b,result);
    printf("diff=%d&%d=%d\n",a,b,a-b);
    printf("multp=%d&%d=%d\n",a,b,a*b);
    div=a/b;
    printf("div=%d&%d=%f\n",a,b,div);

}
