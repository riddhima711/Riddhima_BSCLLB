#include<stdio.h>
void main()
{
    int numbers[10];
    int i;
    printf("Enter 10 integer:\n");
    for(i=0;i<10;i++)
    {
    printf("Enter number %d:", i+1);
    scanf("%d",&numbers[i]);
    }
    printf("The number in reverse order are:\n");
    for(i=9;i>=0;i--)
    {

    printf("%d\n",numbers[i]);
}
}
