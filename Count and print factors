#include<stdio.h>
void main()
{
    int n, count=0;
    printf("Enter the number:");
    scanf("%d",&n);
    printf("Factors of %d are:\n",n);
    for(int i=1;i<=n;i++)
    {
       if(n%i==0)
       {
           printf("%d ", i);
           count++;
       }
    }
    printf("\nTotal number of factors: %d\n", count);
}
